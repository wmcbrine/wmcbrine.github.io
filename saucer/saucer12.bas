370 IF M<0 THEN M=M+220 ELSE IF M>219 THEN M=M-220
380 IF N<0 THEN N=N+180 ELSE IF N>179 THEN N=N-180
390 IF (SK=1) AND ((MO=M) AND (NO=N)) THEN GOTO 430
